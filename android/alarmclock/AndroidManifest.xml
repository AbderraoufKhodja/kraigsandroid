<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.angrydoughnuts.android.alarmclock"
      android:versionCode="1" android:versionName="1.0.0" xmlns:android="http://schemas.android.com/apk/res/android">

  <application android:icon="@drawable/icon" android:label="@string/app_name">
    <activity android:name=".AlarmClockActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER"></category>
      </intent-filter>
    </activity>
    <service android:name="AlarmClockService"></service>
    <activity android:name="AlarmNotificationActivity"></activity>
  </application>

  <uses-sdk android:minSdkVersion="4" />
</manifest> 